{
  "name": "provider-dashboard",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test": "vitest run && npx playwright test",
    "test:e2e": "vitest run --dir ./test/e2e",
    "test:stake": "vitest stake",
    "test:ci": "vitest run --dir ./test/unit-and-integration --coverage",
    "test:ui": "vitest --ui",
    "test:watch": "vitest src",
    "coverage": "vitest run --coverage",
    "lint": "npx eslint src test",
    "lint:fix": "npx eslint --fix src test",
    "format": "npm run prettier && npm run lint:fix",
    "prettier": "npx prettier --write --plugin prettier-plugin-svelte --plugin prettier-plugin-tailwindcss test src",
    "prettier:check:ci": "npx prettier --check --plugin prettier-plugin-svelte --plugin prettier-plugin-tailwindcss test src"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@playwright/test": "^1.48.2",
    "@polkadot/types-codec": "^12.4.1",
    "@sveltejs/adapter-static": "^3.0.4",
    "@sveltejs/kit": "2.5.24",
    "@sveltejs/vite-plugin-svelte": "^3.1.1",
    "@tailwindcss/forms": "^0.5.7",
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/svelte": "^5.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/eslint__js": "^8.42.3",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.8.4",
    "@typescript-eslint/eslint-plugin": "^8.2.0",
    "@typescript-eslint/parser": "^8.2.0",
    "@vitest/coverage-v8": "^2.0.5",
    "@vitest/ui": "^2.0.5",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.9.0",
    "eslint-plugin-svelte": "^2.43.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "^24.1.1",
    "postcss": "^8.4.41",
    "prettier": "^3.3.3",
    "prettier-plugin-svelte": "^3.2.6",
    "prettier-plugin-tailwindcss": "^0.6.6",
    "sass": "^1.77.8",
    "svelte": "^4.2.18",
    "svelte-check": "^3.8.5",
    "svelte-eslint-parser": "^0.41.0",
    "tailwindcss": "^3.4.10",
    "tslib": "^2.6.3",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.2.0",
    "vite": "^5.4.2",
    "vitest": "^2.0.5"
  },
  "type": "module",
  "dependencies": {
    "@frequency-chain/api-augment": "^1.13.2",
    "@polkadot/api": "^13.2.1",
    "@polkadot/extension-dapp": "^0.53.1",
    "@polkadot/extension-inject": "^0.53.1",
    "@polkadot/keyring": "^13.1.1",
    "@polkadot/types": "^13.2.1",
    "@polkadot/util": "^13.1.1",
    "jsdom": "^24.1.1",
    "rxjs": "^7.8.1"
  },
  "stackblitz": {
    "startCommand": "npm run test:ui"
  }
}
